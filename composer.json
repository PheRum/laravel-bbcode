{
    "name": "pherum/laravel-bbcode",
    "description": "Parse your BBCode easy with this library.",
    "keywords": [
        "bbcode",
        "parser",
        "laravel"
    ],
    "homepage": "https://github.com/pherum/laravel-bbcode",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Landberg",
            "email": "joseph.landberg@gmail.com",
            "homepage": "https://github.com/golonka/"
        }
    ],
    "require": {
        "illuminate/support": ">= 6.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">= 8.0"
    },
    "autoload": {
        "psr-4": {
            "PheRum\\BBCode\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PheRum\\BBCode\\BBCodeServiceProvider"
            ],
            "aliases": {
                "BBCode": "PheRum\\BBCode\\Facades\\BBCode"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
